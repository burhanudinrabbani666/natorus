{
  "name": "natorus",
  "version": "1.0.0",
  "description": "project for learning fundamental sass",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "watch:sass": "sass ./sass/main.scss ./css/style.css -w",
    "compile:sass": "sass ./sass/main.scss ./css/style.comp.css",
    "concat:css": "concat -o ./css/style.concat.css ./css/icon.css ./css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' ./css/style.concat.css -o ./css/style.prefixed.css",
    "compress:css": "sass ./css/style.prefixed.css ./css/style.css --style=compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/burhanudinrabbani666/natorus.git"
  },
  "author": "burhanudin rabbani and jonas schedtmann",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/burhanudinrabbani666/natorus/issues"
  },
  "homepage": "https://github.com/burhanudinrabbani666/natorus#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.23",
    "concat": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^11.0.1",
    "sass": "^1.97.2"
  }
}
